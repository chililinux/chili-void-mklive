{
   "action": {
      "default_icon": {
         "16": "img/browsericons/icon16.png",
         "32": "img/browsericons/icon32.png"
      },
      "default_title": "Torrent Scanner"
   },
   "author": "Adaware",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "/js/lib/socket.io.min.js", "/js/systemUtil.js", "/js/functionUtil.js", "/js/contentscript.js", "/js/scraperscript.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": false,
      "css": [ "/css/custom.css" ],
      "js": [ "/js/popup.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "default_locale": "en",
   "description": "__MSG_extShortDesc__",
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "img/icon_128.png",
      "16": "img/icon_16.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "88",
   "name": "Torrent Scanner",
   "optional_permissions": [ "nativeMessaging" ],
   "permissions": [ "storage", "tabs" ],
   "short_name": "Torrent Scanner",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4.0",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "*.html" ]
   }, {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "img/*" ]
   }, {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "js/*" ]
   }, {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "css/*" ]
   } ]
}
